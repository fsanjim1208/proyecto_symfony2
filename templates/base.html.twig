<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            <link href="{{ asset('CSS/Bootstrap/style.css') }}" type="text/css" rel="stylesheet" />
            <link href="{{ asset('CSS/Bootstrap/bootstrap.min.css') }}" type="text/css" rel="stylesheet" />
            <link href="{{ asset('CSS/Bootstrap/animate/animate.css') }}" type="text/css" rel="stylesheet" />
            <link href="{{ asset('CSS/Bootstrap/lightbox/css/lightbox.css') }}" type="text/css" rel="stylesheet" />
            <link href="{{ asset('CSS/Bootstrap/assets/owl.carousel.css') }}" type="text/css" rel="stylesheet" />
            <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet">
            <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css" rel="stylesheet">
        {% endblock %}

        {% block javascripts %}
            <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
            <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js" type="text/javascript"></script>
            <script src="{{ asset('Bootstrap/solartec-1.0.0/lib/wow/wow.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('Bootstrap/solartec-1.0.0/lib/easing/easing.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('Bootstrap/solartec-1.0.0/lib/waypoints/waypoints.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('Bootstrap/solartec-1.0.0/lib/counterup/counterup.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('Bootstrap/solartec-1.0.0/lib/owlcarousel/owl.carousel.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('Bootstrap/solartec-1.0.0/lib/wow/wow.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('Bootstrap/solartec-1.0.0/lib/isotope/isotope.pkgd.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('Bootstrap/solartec-1.0.0/lib/lightbox/js/lightbox.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('Bootstrap/solartec-1.0.0/js/main.js') }}" type="text/javascript"></script>
        {% endblock %}
    </head>
    <body>
        {% block body %}
            
        {% endblock %}
    </body>
</html>
